if ENV["GITHUB_USERNAME"] then
  git_url("https://#{ENV["GITHUB_USERNAME"]}:#{ENV["GITHUB_PASSWORD"]}@github.com/mathieudutour/fastlane-timeline")
else
  git_url("https://github.com/mathieudutour/fastlane-timeline")
end

storage_mode("git")
shallow_clone(true)

type("development") # The default type, can be: appstore, adhoc, enterprise or development

app_identifier(["me.dutour.mathieu.TimeLine"])
# username("user@fastlane.tools") # Your Apple Developer Portal username

